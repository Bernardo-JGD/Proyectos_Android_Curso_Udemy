<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- Color de fondo del borde -->
            <!-- Ancho del borde en dp -->
            <stroke
                android:width="2dp"
                android:color="#000000" /> <!-- Color del borde -->
            <corners android:radius="10dp" /> <!-- Radio de las esquinas en dp -->
        </shape>
    </item>
</layer-list>
